function fibonacci(a: i32): i32
{
	if a <= 0
		return 0;

	if a == 1
		return 1;

	var t1 = 0;
	var t2 = 1;
	var next = t1 + t2;

	for var i: i32 = 3; i < a; i += 1
	{
		t1 = t2;
		t2 = next;
		next = t1 + t2;
	}

	return next;
}

